<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="description" content="A front-end template that helps you build fast, modern mobile web apps.">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0">

  <title>{% set title_s %}{% block title %}{% endblock %}・ SAFE ・ Codelco Ventanas{% endset %}</title>

  {#
  <!-- Add to homescreen for Chrome on Android -->
  <meta name="mobile-web-app-capable" content="yes">
  <link rel="icon" sizes="192x192" href="images/android-desktop.png">
  

  <!-- Add to homescreen for Safari on iOS -->
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">
  <meta name="apple-mobile-web-app-title" content="Material Design Lite">
  <link rel="apple-touch-icon-precomposed" href="images/ios-desktop.png">
  #}


  <link rel="shortcut icon" href="{{ url_for('static', filename='img/faviconLabs.png')}}">

  <link rel="stylesheet"href="{{ url_for('static', filename='css/material_fonts.css')}}">
  <link rel="stylesheet" href="{{ url_for('static', filename='css/material_icons.css')}}">
  <link rel="stylesheet" href="{{ url_for('static', filename='css/material.cyan-light_blue.min.css')}}">

  <link rel="stylesheet" href="{{ url_for('static', filename='css/styles.css')}}">
  <link rel="stylesheet" href="{{ url_for('static', filename='css/mdl-expansion.css')}}">


  {% block extra_style_files %}{% endblock %}
  {% block extra_style_inline %}{% endblock %}
  {% block header_scripts %}{% endblock %}

</head>

<body>
  <div class="demo-layout mdl-layout mdl-js-layout mdl-layout--fixed-drawer mdl-layout--fixed-header">

    <header class="demo-header mdl-layout__header mdl-color--grey-100 mdl-color-text--grey-600">
      <div class="mdl-layout__header-row">
        <span class="mdl-layout-title">{{ title_s }}</span>
      </div>
    </header>
    <div class="demo-drawer mdl-layout__drawer mdl-color--blue-grey-900 mdl-color-text--blue-grey-50">
      <nav class="demo-navigation mdl-navigation mdl-color--blue-grey-800">
        <a class="mdl-navigation__link" href="/"><i class="mdl-color-text--blue-grey-400 material-icons"
            role="presentation">home</i>Home</a>
        <a class="mdl-navigation__link" href="/live"><i class="mdl-color-text--blue-grey-400 material-icons"
            role="presentation">live_tv</i>Transmision en vivo</a>
        <a class="mdl-navigation__link" href="/registry"><i class="mdl-color-text--blue-grey-400 material-icons"
            role="presentation">warning</i>Registro de Eventos</a>
        <a class="mdl-navigation__link" href="/stats"><i class="mdl-color-text--blue-grey-400 material-icons"
            role="presentation">assessment</i>Estadisticas</a>
      </nav>
    </div>

    <main class="mdl-layout__content mdl-color--grey-100">
      <div class="mdl-grid demo-content">
        {% block content %}{% endblock %}
      </div>
    </main>
  </div>

  <script src="{{ url_for('static', filename='js/material.min.js')}}"></script>

  {% block postbody_scripts %}{% endblock %}

</body>

</html>
